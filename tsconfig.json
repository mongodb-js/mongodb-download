{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "strictNullChecks": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "sourceMap": true,
        "noLib": false,
        "outDir": "./built",
        "lib": ["es2015.promise", "es5"]
    },
    "include": [
        "src/**/*"
    ],
    "typeAcquisition": {
        "enable": true
    },
    "exclude": [
        "node_modules",
        "typings",
        "**/*.spec.ts"
    ]
}